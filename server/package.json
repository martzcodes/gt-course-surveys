{
  "name": "gt-course-surveys-server",
  "version": "1.0.11",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "rules:staging": "firebase use staging && echo Compiling... && blaze rules.yaml &> /dev/null && firebase deploy --only database && gulp clean:rules",
    "rules:production": "firebase use production && echo Compiling... && blaze rules.yaml &> /dev/null && firebase deploy --only database && gulp clean:rules",
    "imports:staging": "export NODE_ENV=staging && npm run build &> /dev/null && node dist/server/base/data/imports/imports.js && unset NODE_ENV",
    "imports:production": "export NODE_ENV=production && npm run build &> /dev/null && node dist/server/base/data/imports/imports.js && unset NODE_ENV",
    "clean": "gulp clean",
    "eslint": "gulp eslint",
    "build": "gulp build",
    "test": "gulp test",
    "serve": "gulp serve"
  },
  "dependencies": {
